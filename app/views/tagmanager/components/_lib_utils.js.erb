var SpymasterUtils = (function() {
  /*
   * Each tracking request should have an unique id
   * This is a very simple mechanism to get it
   */
  var request_id_auto_increment = 0;

  var internal_id_for_tracking_request = function() {
    time = new Date().getTime();
    cid = request_id_auto_increment++;
    return (time + '-' + cid);
  };

  return {
    id_for_tracking_request: function() {
      return internal_id_for_tracking_request();
    },
  }
})();